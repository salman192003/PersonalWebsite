{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "http://localhost:5000/api/:path*"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "framework": "create-react-app",
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/logo192.png",
            "dest": "/logo192.png"
        },
        {
            "src": "/logo512.png",
            "dest": "/logo512.png"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}